{
  "name": "image-index-slider",
  "version": "1.0.0",
  "description": "CommonSpot standard element Image Index Slider render handler",
  "repository": "https://sayre_ts@bitbucket.org/sayre_ts/image-index-slider.git",
  "license": "ISC",
  "author": {
    "name": "Todd Sayre",
    "email": "sayre_ts@mercer.edu"
  },
  "devDependencies": {
    "autoprefixer": "^7.0.1",
    "babel-preset-latest": "^6.24.1",
    "babelify": "^7.3.0",
    "babili": "0.0.12",
    "browserify": "^14.3.0",
    "cpy-cli": "^1.0.1",
    "cssnano-cli": "^1.0.5",
    "gh-markdown-cli": "^0.2.0",
    "marked": "^0.3.6",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.5.2",
    "npm-run-all": "^4.0.2",
    "postcss-cli": "^4.0.0",
    "postcss-object-fit-images": "^1.1.2",
    "rimraf": "^2.6.1",
    "sass-bem": "^2.6.5"
  },
  "scripts": {
    "prebuild:cf": "mkdirp dist/cf",
    "prebuild:css": "mkdirp dist/css",
    "prebuild:js": "mkdirp dist/js",
    "prebuild": "run-s clean && run-p prebuild:*",
    "build:cf": "cpy **/*.{cfc,cfm} ../dist/cf --cwd=cf --parents",
    "build:css:theme-default:scss": "node-sass scss/themes/default/index.scss scss/themes/default/compiled.css",
    "build:css:theme-default:postcss": "postcss scss/themes/default/compiled.css -u autoprefixer -o scss/themes/default/compiled.css",
    "build:css:theme-default": "run-s build:css:theme-default:*",
    "build:css:core:scss": "node-sass scss/index.scss dist/css/image-index-slider.css",
    "build:css:core:postcss": "postcss dist/css/image-index-slider.css -u autoprefixer -o dist/css/image-index-slider.css",
    "build:css:core": "run-s build:css:core:*",
    "build:css": "run-s build:css:*",
    "build:js": "browserify js/index.js -o dist/js/image-index-slider.js -t [ babelify --presets [ latest ] ]",
    "build:test:css:node-sass": "node-sass test/testing.scss test/testing.css",
    "build:test:css:postcss": "postcss test/testing.css -u autoprefixer -o test/testing.css",
    "build:test:css": "run-s build:test:css:*",
    "build": "run-p build:* readme2html",
    "postbuild:css:clean": "rimraf scss/themes/default/compiled.css",
    "postbuild:css:minification": "cssnano dist/css/image-index-slider.css dist/css/image-index-slider.min.css",
    "postbuild:css": "run-p postbuild:css:*",
    "postbuild:js": "babili dist/js/image-index-slider.js -o dist/js/image-index-slider.min.js",
    "postbuild:test-dist": "cpy **/*.* ../test/dist --cwd=dist --parents",
    "postbuild": "run-p postbuild:*",
    "clean:dist": "rimraf dist && mkdirp dist",
    "clean:test-dist": "rimraf test/dist && mkdirp test/dist",
    "clean": "run-p clean:*",
    "readme2html": "marked Readme.md > test/readme.html",
    "test": "live-server test"
  },
  "babel": {}
}
